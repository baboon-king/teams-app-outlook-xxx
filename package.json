{
  "name": "teams-app-outlook-xxx",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.11",
  "config": {
    "app_to_debug": "outlook",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "lint:office": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "signin": "office-addin-dev-settings m365-account login",
    "signout": "office-addin-dev-settings m365-account logout",
    "prettier": "office-addin-lint prettier",
    "start": "office-addin-debugging start appPackage/manifest.json",
    "stop": "office-addin-debugging stop appPackage/manifest.json",
    "validate": "office-addin-manifest validate appPackage/manifest.json"
  },
  "dependencies": {
    "core-js": "3.9.1",
    "devextreme": "23.1",
    "devextreme-vue": "23.1",
    "pinia": "2.1.6",
    "regenerator-runtime": "0.13.7",
    "vue": "3.3.4",
    "vue-router": "4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.3.3",
    "@tsconfig/node18": "18.2.2",
    "@types/node": "18.17.15",
    "@types/office-js": "^1.0.256",
    "@types/office-runtime": "^1.0.23",
    "@vitejs/plugin-vue": "4.3.4",
    "@vitejs/plugin-vue-jsx": "3.0.2",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/tsconfig": "0.4.0",
    "acorn": "^8.5.0",
    "eslint": "8.49.0",
    "eslint-plugin-office-addins": "^2.1.5",
    "eslint-plugin-vue": "9.17.0",
    "npm-run-all": "4.1.5",
    "office-addin-cli": "^1.5.5",
    "office-addin-debugging": "^5.0.5",
    "office-addin-dev-certs": "^1.11.3",
    "office-addin-lint": "^2.2.5",
    "office-addin-manifest": "^1.12.3",
    "office-addin-prettier-config": "^1.2.0",
    "os-browserify": "^0.3.0",
    "prettier": "3.0.3",
    "process": "^0.11.10",
    "typescript": "~5.1.6",
    "unocss": "0.55.7",
    "vite": "4.4.9",
    "vue-tsc": "1.8.11"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "ie 11"
  ]
}
